{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/build/*",
    "/static/*",
    "/node_modules/*",
    "/.next/*",
    "/.git/*"
  ],
  "routes": [
    {
      "src": "/_next/static/chunks/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/_next/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/_next/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ]
} 